<h1>Nuevo Post</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<%= form_with(model: @post, local: true) do |form| %>
    <div>
      <%= form.label :description %>
      <%= form.text_area :description %>
    </div>
    <div>
      <%= form.label :image %>
      <%= form.file_field :image %>
    </div>
    <div>
      <%= form.submit 'Crear Post' %>
    </div>
<% end %>
