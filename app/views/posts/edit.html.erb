<li class="post-card">
<h1>Editar Post</h1>
<%= form_with(model: @post, local: true) do |form| %>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>
  <div>
    <%= form.submit 'Actualizar Post' %>
  </div>
<% end %>
</li>